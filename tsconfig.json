{
  "compilerOptions": {
    "outDir": "static/dist",
    "module": "es2015",
    "target": "es2017",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    //    "sourceMap": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "removeComments": true,
    "watch": true,
    "baseUrl": "src"
  },
  "include": [
    "static/src",
    "node_modules/handlebars/types/"
  ]
}